{
  "name": "@rarible/sdk",
  "version": "0.3.0",
  "homepage": "",
  "license": "ISC",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "run-s create-validators validators-lint-fix build-ts",
    "build-babel": "babel --extensions \".ts,.tsx\" ./src --out-dir build && tsc --project tsconfig-build.json --emitDeclarationOnly && yarn run replace-mjs",
    "build-ts": "tsc --project tsconfig-build.json",
    "clean": "rimraf ./build",
    "create-validators": "create-validator-ts src/**/*.type.ts",
    "replace-mjs": "find ./build -type f -name \"*.js\" -exec sed -i '' 's#./jsbi.mjs#jsbi#' {} \\;",
    "test": "jest --runInBad --silent",
    "validators-lint-fix": "eslint --ext type.validator.ts . --fix",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",

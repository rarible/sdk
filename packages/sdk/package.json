    "ethereumjs-wallet": "^1.0.2",
    "jsbi": "^3.2.5",
    "node-fetch": "2.6.1",
    "tezos-sdk-module": "^0.0.5"
  },
  "devDependencies": {
    "@airgap/beacon-sdk": "^2.3.2",

    "jest-jasmine2": "^29.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "dotenv": "16.0.0",
    "jest": "^29.4.1",
    "jest-allure": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.5",

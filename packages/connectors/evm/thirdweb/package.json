{
  "name": "@rarible/connector-thirdweb",
  "version": "0.13.68-fix.43",
  "homepage": "https://docs.rarible.org/reference/getting-started",
  "license": "ISC",
  "exports": {
    ".": {
      "types": {
        "require": "./build/index.d.ts",
        "import": "./esm/index.d.ts"
      },
      "require": "./build/index.js",
      "import": "./esm/index.js",
      "default": "./build/index.js"
    },
    "./package.json": "./package.json",
    "./build/": "./build/",
    "./esm/": "./esm/",
    "./*": {
      "types": {
        "require": "./build/*.d.ts",
        "import": "./esm/*.d.ts"
      },
      "require": "./build/*.js",
      "import": "./esm/*.js",
      "default": "./build/*.js"
    }
  },
  "main": "./build/index.js",
  "module": "./esm/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "esm"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "clean": "run-s clean:*",
    "clean:build": "rimraf ./build ./esm",
    "clean:tsbuildinfo": "rimraf ./tsconfig.tsbuildinfo",
    "verify": "tsc"
  },
  "dependencies": {
    "@rarible/connector": "^0.13.68-fix.43",
    "bignumber.js": "^9.1.2",
    "thirdweb": "^5.24.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "peerDependenciesMeta": {},
  "publishConfig": {
    "access": "public"
  }
}

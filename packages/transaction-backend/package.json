{
  "name": "@rarible/sdk-transaction-backend",
  "version": "0.13.93",
  "private": true,
  "homepage": "",
  "license": "ISC",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "run-s clean build:ts copy-files",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "run-s clean:*",
    "clean:build": "rimraf ./build",
    "clean:tsbuildinfo": "rimraf ./tsconfig.tsbuildinfo",
    "copy-files": "cpy ./src/openapi.yml ./build/ --flat",
    "get-buy-tx": "npx ts-node scripts/gettxbuy.ts",
    "get-orders": "npx ts-node scripts/getorders.ts",
    "serve": "nodemon",
    "start": "node ./build/index.js",
    "test": "jest --runInBand --coverage --coverageReporters=lcov",
    "test:no-coverage": "jest --runInBand",
    "verify": "tsc"
  },
  "dependencies": {
    "@rarible/protocol-ethereum-sdk": "^0.13.93",
    "@rarible/web3-ethereum": "^0.13.93",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "isomorphic-fetch": "^3.0.0",
    "redoc-express": "^1.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@rarible/ethereum-sdk-test-common": "^0.13.93",
    "@types/body-parser": "^1.19.2",
    "@types/express": "4.17.11",
    "@types/isomorphic-fetch": "~0.0.39",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.1",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  },
  "peerDependencies": {
    "web3": ">=1.5.0 <2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

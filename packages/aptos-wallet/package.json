{
  "name": "@rarible/aptos-wallet",
  "version": "0.13.85",
  "keywords": [
    "solana",
    "web3"
  ],
  "license": "Apache-2.0",
  "exports": {
    "./package.json": "./package.json",
    "./build/": "./build/",
    "./esm/": "./esm/",
    ".": {
      "types": {
        "require": "./build/index.d.ts",
        "import": "./esm/index.d.ts"
      },
      "require": "./build/index.js",
      "import": "./esm/index.js",
      "default": "./build/index.js"
    },
    "./*": {
      "types": {
        "require": "./build/*.d.ts",
        "import": "./esm/*.d.ts"
      },
      "require": "./build/*.js",
      "import": "./esm/*.js",
      "default": "./build/*.js"
    }
  },
  "main": "./build/index.js",
  "module": "./esm/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "umd",
    "esm"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "run-s clean:*",
    "clean:build": "rimraf ./build ./esm",
    "clean:tsbuildinfo": "rimraf ./tsconfig.tsbuildinfo",
    "test": "echo \"There's no tests yet\"",
    "verify": "tsc"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.5.1",
    "@aptos-labs/wallet-adapter-core": "~4.8.0",
    "@rarible/sdk-common": "^0.13.85",
    "@rarible/types": "~0.10.0-beta.6"
  },
  "peerDependencies": {
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "8ef3989b1aff76a6345deab53a76df3480d8cf46"
}

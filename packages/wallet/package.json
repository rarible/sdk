{
  "name": "@rarible/sdk-wallet",
  "version": "0.13.68-fix.48",
  "homepage": "https://github.com/rarible/sdk/",
  "license": "ISC",
  "exports": {
    "./package.json": "./package.json",
    "./build/": "./build/",
    "./esm/": "./esm/",
    ".": {
      "types": {
        "require": "./build/index.d.ts",
        "import": "./esm/index.d.ts"
      },
      "require": "./build/index.js",
      "import": "./esm/index.js",
      "default": "./build/index.js"
    },
    "./*": {
      "types": {
        "require": "./build/*.d.ts",
        "import": "./esm/*.d.ts"
      },
      "require": "./build/*.js",
      "import": "./esm/*.js",
      "default": "./build/*.js"
    }
  },
  "main": "./build/index.js",
  "module": "./esm/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "umd",
    "esm"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "webpack",
    "clean": "run-s clean:*",
    "clean:build": "rimraf ./build ./esm ./umd",
    "clean:tsbuildinfo": "rimraf ./tsconfig.tsbuildinfo",
    "test": "jest --runInBand --forceExit --silent",
    "verify": "tsc"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.4.0",
    "@rarible/aptos-wallet": "^0.13.68-fix.48",
    "@rarible/ethereum-provider": "^0.13.68-fix.48",
    "@rarible/ethers-ethereum": "^0.13.68-fix.48",
    "@rarible/fcl-types": "~0.5.56",
    "@rarible/flow-sdk": "~0.5.78-fix.6",
    "@rarible/solana-common": "^0.13.68-fix.48",
    "@rarible/solana-wallet": "^0.13.68-fix.48",
    "@rarible/tezos-sdk": "0.1.46",
    "@rarible/types": "~0.9.30",
    "@rarible/web3-ethereum": "^0.13.68-fix.48",
    "text-encoding": "~0.7.0"
  },
  "devDependencies": {
    "@rarible/ethereum-sdk-test-common": "^0.13.68-fix.48",
    "@types/text-encoding": "~0.0.39",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0"
  },
  "peerDependencies": {
    "@rarible/api-client": "0.18.0-alpha.2",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@rarible/sdk-wallet",
  "version": "0.13.68-fix.23",
  "homepage": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types": {
        "require": "./build/index.d.ts",
        "import": "./esm/index.d.ts"
      },
      "require": "./build/index.js",
      "import": "./esm/index.js",
      "default": "./build/index.js"
    },
    "./build/": "./build/",
    "./*": {
      "types": {
        "require": "./build/*.d.ts",
        "import": "./esm/*.d.ts"
      },
      "require": "./build/*.js",
      "import": "./esm/*.js",
      "default": "./build/*.js"
    }
  },
  "main": "build/index.js",
  "module": "esm/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "umd",
    "esm"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:umd": "webpack",
    "clean": "run-s clean:*",
    "clean:build": "rimraf ./build",
    "clean:tsbuildinfo": "rimraf ./tsconfig.tsbuildinfo",
    "clean:umd": "rimraf ./umd",
    "test": "jest --runInBand --forceExit --silent",
    "verify": "tsc"
  },
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.4.0",
    "@imtbl/imx-sdk": "^2.1.1",
    "@rarible/api-client": "0.16.5-alpha.10",

    "@babel/preset-typescript": "^7.14.5",
    "@rarible/ethereum-sdk-test-common": "^0.13.68-fix.23",
    "@types/text-encoding": "~0.0.39",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0"
  },
  "peerDependencies": {
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
